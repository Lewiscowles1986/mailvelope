{
  "version": "@@mvelo_version",
  "watch_list": [
    {
      "active": true,
      "site": "All",
      "https_only": false,
      "prevent_landingpage": true,
      "frames": [
        {
          "scan": true,
          "frame": "*.com",
          "api": false
        }      ]
    }
  ],
  "preferences": {
    "security": {
      "display_decrypted": "inline",
      "password_cache": true,
      "password_timeout": 30,
      "hide_armored_header": false
    },
    "general": {
      "auto_add_primary": true,
      "auto_sign_msg": false,
      "prefer_gnupg": true
    },
    "keyserver": {
      "hkp_base_url": "https://keyserver.ubuntu.com",
      "hkp_server_list": [
        "https://keyserver.ubuntu.com",
        "http://pool.sks-keyservers.net:11371",
        "https://pgp.mit.edu",
        "https://keys.mailvelope.com"
      ],
      "mvelo_tofu_lookup": true,
      "wkd_lookup": true,
      "autocrypt_lookup": true,
      "wkd_blacklist": [
        "gmail\\..*",
        "googlemail\\..*",
        "gmx\\..*",
        "outlook\\..*",
        "hotmail\\..*",
        "web.de",
        "yahoo\\..*"
      ]
    }
  }
}
